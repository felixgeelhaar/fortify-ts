{
  "name": "@fortify-ts/logging",
  "version": "0.2.0",
  "description": "Structured logging utilities for fortify-ts resilience patterns",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fortify-ts/core": "workspace:*"
  },
  "peerDependencies": {
    "pino": "^8.0.0 || ^9.0.0",
    "zod": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "pino": {
      "optional": true
    }
  },
  "devDependencies": {
    "fast-check": "^4.5.3",
    "pino": "^10.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felixgeelhaar/fortify-ts.git",
    "directory": "packages/logging"
  },
  "homepage": "https://github.com/felixgeelhaar/fortify-ts/tree/main/packages/logging#readme",
  "bugs": {
    "url": "https://github.com/felixgeelhaar/fortify-ts/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "resilience",
    "logging",
    "pino",
    "fortify"
  ],
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
