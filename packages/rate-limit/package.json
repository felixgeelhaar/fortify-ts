{
  "name": "@fortify-ts/rate-limit",
  "version": "0.1.0",
  "description": "Token bucket rate limiter for @fortify-ts",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fortify-ts/core": "workspace:*",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "keywords": [
    "rate-limit",
    "token-bucket",
    "throttle",
    "resilience"
  ],
  "license": "MIT"
}
