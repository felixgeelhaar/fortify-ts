{
  "name": "@fortify-ts/metrics",
  "version": "0.1.5",
  "description": "Prometheus metrics for fortify-ts resilience patterns",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fortify-ts/core": "workspace:*"
  },
  "peerDependencies": {
    "prom-client": "^14.0.0 || ^15.0.0"
  },
  "peerDependenciesMeta": {
    "prom-client": {
      "optional": true
    }
  },
  "devDependencies": {
    "prom-client": "^15.1.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felixgeelhaar/fortify-ts.git",
    "directory": "packages/metrics"
  },
  "homepage": "https://github.com/felixgeelhaar/fortify-ts/tree/main/packages/metrics#readme",
  "bugs": {
    "url": "https://github.com/felixgeelhaar/fortify-ts/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "resilience",
    "metrics",
    "prometheus",
    "fortify"
  ],
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
